    "typescript": "^2.0.0",
    "umd-require-webpack-plugin": "0.0.1",
    "url-loader": "^0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-dev-server": "1.16.2",
