    "ng2-page-scroll": "^3.1.5",
    "ng2-translate": "^3.1.3",
    "rxjs": "5.0.0-beta.12",
    "source-map-loader": "^0.1.5",
    "tslint-loader": "^2.1.5",
    "zone.js": "~0.6.25"
  },
